{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron Main",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "cwd": "${workspaceRoot}/desktop",
      "args": [
        "${workspaceRoot}/build/desktop.js",
        "--remote-debugging-port=9333"
      ],
      "protocol": "inspector"
    }
  ]
}
